{
  "name": "open-patcher",
  "version": "1.0.0",
  "description": "A simple patcher for javascript objects",
  "author": "abelpz",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "pnpm --package=typescript dlx tsc",
    "lint": "eslint '*/**/*.{ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{ts,tsx}' --fix",
    "find-deadcode": "ts-prune -s .test.ts?",
    "prepublish": "pnpm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abelpz/open-patcher.git"
  },
  "bugs": {
    "url": "https://github.com/abelpz/open-patcher/issues"
  },
  "homepage": "https://github.com/abelpz/open-patcher",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.4.3"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "diff",
    "patcher",
    "patch",
    "object",
    "open",
    "json",
    "patch",
    "http",
    "rest",
    "jsonpointer"
  ]
}
